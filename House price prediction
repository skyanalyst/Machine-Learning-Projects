    {
      "cell_type": "code",
      "source": [
        "# Create inputs and Targets\n",
        "input_cols = list(raw_df.columns)[1:-1]\n",
        "target_col = 'SalePrice'\n",
        "inputs_df = raw_df[input_cols].copy()\n",
